datasource db {
  provider = "mysql"
  url      = env("MYSQL_URI")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  price       Float
  createdAt   DateTime @default(now())

  @@map("products")
}
